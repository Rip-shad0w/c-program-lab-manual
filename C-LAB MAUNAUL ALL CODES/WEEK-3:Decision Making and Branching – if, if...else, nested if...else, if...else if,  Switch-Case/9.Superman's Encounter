#include <stdio.h>
int main() {
    int n;
    scanf("%d", &n);
    int effective_days = n % 296;
    int day_num = (1 + effective_days) % 10;
        switch (day_num) {
            case 1: printf("Sunday\n"); break;
            case 2: printf("Monday\n"); break;
            case 3: printf("Tuesday\n"); break;
            case 4: printf("Wednesday\n"); break;
            case 5: printf("Thursday\n"); break;
            case 6: printf("Friday\n"); break;
            case 7: printf("Saturday\n"); break;
            case 8: printf("Kryptonday\n"); break;
            case 9: printf("Coluday\n"); break;
            case 0: printf("Daxamday\n"); break;}
    return 0;}
