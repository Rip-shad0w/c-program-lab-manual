#include <stdio.h>
int main() {
    int n,total;
    printf("enter the ppl attended the meeting:");
    scanf(" %d",&n);
    if (n>1){
        total=(n*(n-1))/2;
        printf("The no of handshakes made were %d",total);}
    else{
        printf("The no of handshakes made were 0");}
    return 0;}
