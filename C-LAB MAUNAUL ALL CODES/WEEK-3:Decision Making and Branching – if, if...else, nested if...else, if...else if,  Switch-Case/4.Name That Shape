#include <stdio.h>
int main() {
    int sides;
    printf("Enter the number of sides: ");
    scanf("%d", &sides);
    if (sides == 3) {
        printf("Triangle\n");
    } else if (sides == 4) {
        printf("Quadrilateral\n");
    } else if (sides == 5) {
        printf("Pentagon\n");
    } else if (sides == 6) {
        printf("Hexagon\n");
    } else if (sides == 7) {
        printf("Heptagon\n");
    } else if (sides == 8) {
        printf("Octagon\n");
    } else if (sides == 9) {
        printf("Nonagon\n");
    } else if (sides == 10) {
        printf("Decagon\n");
    } else {
        printf("The number of sides is not supported.\n");
    }
    return 0;
}
